package 集合;

import java.util.ArrayList;
import java.util.Collection;

public class CollectionTest {

	public static void main(String[] args) {
		
	Collection c=new ArrayList<>();
	c.add(1);
	c.add(2);
	
	
	
	Collection c1=new ArrayList<>();
	c1.add(1);
	
	c.retainAll(c1);
	c.add(c1);
	System.out.println(c);
	}
	
}
